<body>
    <script>
        let array = [];
        let n = 0;
        for (let i = 0; i < 10; i++) {
            array.push(parseInt(prompt(`Nhập phần tử thứ ${i + 1}:`)));
            if (array[i]>=10) {
                n++
            }
        }
        console.log(`Số lượng phần tử lớn hơn hoặc bằng 10: ${n}`);
    </script>
</body>
</html>
