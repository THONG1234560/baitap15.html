<body>
    <script>
        let string = "-12 3 -45 6 -7 8 -9";
        let result = 0;
        let numbers = string.split(" ");
        for (let i = 0; i < numbers.length; i++) {
            if (numbers[i] < 0) {
                result++;
            }
        }
        console.log("Số lượng số nguyên âm trong chuỗi:", result);
    </script>
</body>
